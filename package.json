{
  "name": "@waynecodez/wrtc-stream",
  "author": "@waynecodez",
  "version": "0.0.3",
  "proxy": "http://localhost:8080",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "scripts": {
    "start": "react-scripts start",
    "serve": "node server.js",
    "dev": "nodemon server.js",
    "prettier:check": "prettier --check \"**/*.+(ts|tsx|js|jsx|json|css|md)\"",
    "prettier:fix": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|css|md)\"",
    "prepare": "husky install",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:up": "docker-compose up --force-recreate -V",
    "docker:di:tag": "docker tag react-video-image mrwaynerp/react-video",
    "docker:di:push": "docker push mrwaynerp/react-video",
    "docker:splash": "npm run build && docker-compose build && npm run docker:di:tag && npm run docker:di:push",
    "build:package": "vite build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "esbuild": "^0.17.19",
    "eventemitter3": "^5.0.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.8",
    "web-vitals": "^2.1.4",
    "webrtc-adapter": "^8.2.0"
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module"
}
